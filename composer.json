{
  "name": "tourze/symfony-command-profile-bundle",
  "description": "A Symfony bundle that profiles and outputs the runtime of console commands",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "nesbot/carbon": "^2.72 || ^3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\CommandProfileBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\CommandProfileBundle\\Tests\\": "tests"
    }
  }
}